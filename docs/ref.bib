journal = {Epidemiologic Reviews},
keywords = {accidental falls,burns,developing countries,drowning,motor vehicles,poisoning,wounds and injuries},
mendeley-groups = {taft-protocol},
number = {1},
pages = {110--120},
pmid = {20570956},
title = {{The global burden of unintentional injuries and an agenda for progress}},
volume = {32},
