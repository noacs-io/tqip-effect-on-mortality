@article{10.1038/s41591-021-01614-0,
year = {2022},
title = {{AI in health and medicine}},
author = {Rajpurkar, Pranav and Chen, Emma and Banerjee, Oishi and Topol, Eric J.},
journal = {Nature Medicine},
