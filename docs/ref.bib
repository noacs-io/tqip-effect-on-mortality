keywords = {Mortality,Quality improvement,Trauma},
mendeley-groups = {taft-protocol},
number = {5},
pages = {830--834},
pmid = {24290523},
publisher = {Elsevier Ltd},
title = {{A trauma quality improvement programme associated with improved patient outcomes: 21 years of experience at an Australian Major Trauma Centre}},
